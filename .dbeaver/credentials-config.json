{
  "connections": {
    "PropPilot-PostgreSQL": {
      "provider": "postgresql",
      "driver": "postgresql",
      "name": "PropPilot PostgreSQL",
      "save-password": true,
      "configuration": {
        "host": "localhost",
        "port": "5432",
        "database": "proppilot",
        "url": "jdbc:postgresql://localhost:5432/proppilot",
        "configurationType": "MANUAL",
        "type": "dev",
        "provider-properties": {
          "@dbeaver-show-non-default-db@": "false",
          "@dbeaver-show-template-db@": "false",
          "@dbeaver-show-unavailable-db@": "false"
        },
        "auth-model": "native",
        "handlers": {}
      }
    }
  }
}
